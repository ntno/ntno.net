    <div class="image-grid">
      <% for( var i=0; i < details['pics'].length; i++) { %>
          <img src="<%= details['pics'][i].imgsrc %>"
                  alt="<%= details['pics'][i].imgalt %>"
                  title="<%= details['pics'][i].imgtitle %>"
                  width="<%= details['width'] %>"
                  height="<%= details['height'] %>" >
      <% } %>
      <% for( var i=0; i < details['vids'].length; i++) { %>
          <video 
              width="<%= details['width'] %>"
              height="<%= details['height'] %>"
              controls muted>
              <source src="<%= details['vids'][i].vidsrc %>" type="video/mp4">
              Your browser does not support the video tag.
          </video>
      <% } %>
  </div>



